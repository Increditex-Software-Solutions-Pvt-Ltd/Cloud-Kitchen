    <main>
        <section class="mainbanner position-relative">
            <img src="/user/img/meinbanner.jpg" class="w-100 vh-100" alt="">
            <div class="overlay"></div>
            <div class="banner-content text-white">
                <h2 class="display-3 animation-fade"><%= heading%></h2>
            </div>
        </section>
        <section class="intro mb-5">
            <div class="row w-100 d-flex align-items-center">
                <div class="col-md-6 col-sm-12 col-12">
                    <div class="bg-main-gred intotop text-white p-5 p-md-5">
                        <div class="container">
                            <h3 class="my-4"><%= serviceheading%></h3>
                            <p class="normal" style="text-align: justify;"><%= servicedesc%></p>
                        </div>
                    </div>
                    <div class="bg-black intodown text-white p-5 p-md-5">
                        <div class="container">
                            <p class="normal" style="text-align: justify;"><%=serviceshortinfo%></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12 col-12">
                    <div class="px-2 px-sm-1 px-md-5">
                        <div class="container">
                            <h4 class="my-4 mb-my-3">EXPLORING DISHES</h4>
                            <div class="row menuslider d-flex justify-content-center">
                                <% popularDishes.forEach(imageURL => { %>
                                    <div class="col-12 col-sm-12">
                                        <div class="menuimg">
                                            <img src="<%= imageURL %>" class="img-fluid" alt="">
                                        </div>
                                    </div>
                                <% }); %>
                            </div>

                            <div class="videobox" data-video-id="s2BTyC4O_1U">
                                <img src="<%=youtubethumbnail%>" class="img-fluid thumbnail"
                                    alt="">
                                <div class="boxhover">
                                </div>
                                <div class="play-icon">
                                    <img src="/user/img/play-button.png" class="img-fluid" width="70px" alt="">
                                </div>
                                <div class="video-iframe">
                                    <iframe 
                                        src="<%=youtubelink%>"
                                        title="YouTube video player" frameborder="0"
                                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                                        allowfullscreen></iframe>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
            </div>
        </section>

      
        <section class="topdishes my-5 py-5 bg-semilight">
            <div class="container">
                <h4 class="text-dark  text-center my-4">OUR TOP DISHES</h4>

                <div id="topdishes" class="carousel slide" data-bs-ride="carousel">
                    <div class="carousel-inner">
                        <% topdishes.forEach((dish, index) => { %>
                            <div class="carousel-item <%= index === 0 ? 'active' : '' %>">
                                <div class="row d-flex align-items-center justify-content-center">
                                    <div class="col-md-7 col-sm-12 col-12">
                                        <div class="dishtop d-flex justify-content-center ms-4">
                                            <img src="<%= dish.dishimage %>" class="img-fluid" alt="<%= dish.name %>">
                                        </div>
                                    </div>
                                    <div class="col-md-5 col-sm-12 col-12">
                                        <div class="dishspeciality d-flex justify-content-center flex-column">
                                            <h3 class="text-dark text-center text-md-start fw-normal"><%= dish.dishname %></h3>
                                            <span class="text-cement text-center text-md-start fs-4"><%= 'Rs.' + dish.dishprice %></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <% }); %>
                    </div>
                    <button class="carousel-control-prev" type="button" data-bs-target="#topdishes"
                        data-bs-slide="prev">
                        <i class="bi bi-arrow-left-circle fs-2 text-dark"></i>
                        <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next" type="button" data-bs-target="#topdishes"
                        data-bs-slide="next">
                        <i class="bi bi-arrow-right-circle fs-2 text-dark"></i>
                        <span class="visually-hidden">Next</span>
                    </button>
                </div>
            </div>
        </section>


